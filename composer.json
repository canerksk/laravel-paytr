{
    "name": "canerksk/laravel-paytr",
    "license": "MIT",
    "type": "library",
    "description": "Laravel Paytr Wrapper",
    "authors": [
        {
            "name": "Gizem Sever",
            "email": "gizemsever68@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0|^8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/support": "^8|^9|^10|^11",
        "illuminate/collections": "^8|^9|^10|^11"
    },
    "autoload": {
        "psr-4": {
            "Gizemsever\\LaravelPaytr\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gizemsever\\LaravelPaytr\\PaytrServiceProvider"
            ],
            "aliases": {
                "Paytr": "Gizemsever\\LaravelPaytr\\PaytrFacade"
            }
        }
    }
}
